<?php

$string_one = "you have teeth";
$string_two = "toads are nice";
$string_three = "brown is my favorite color"; // SUrya GAMa

// 2255201045
function convertToQuestion(&$s)
{
    $s = "Do you think " . $s . "?\n";
}
// Kelas A

convertToQuestion($string_one);
convertToQuestion($string_two);
convertToQuestion($string_three);

echo $string_one;
echo $string_two;
echo $string_three;



