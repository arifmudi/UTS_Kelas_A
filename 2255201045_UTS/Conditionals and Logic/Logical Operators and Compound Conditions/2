<?php
namespace Codecademy;

function willWeEat($meal, $isHungry)
{
    if ($meal == "dessert" || $isHungry) {
        return "Yum. Thanks!";
    } else {
        return "No thanks. We're not hungry.";
    }
}
echo willWeEat("dessert", false);
echo "\n";
echo willWeEat("lunch", false);
