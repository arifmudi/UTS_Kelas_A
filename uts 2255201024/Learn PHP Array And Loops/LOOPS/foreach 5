<?php
$scores = [
  "zilla" => 80,
  "ajill" => 95,
  "alhashir" => 99,
  "moly" => 91,
  "jilla" => 99
];
//Nim: 2255201024
foreach ($scores as $score) {
  echo "Someone received a score of $score.\n";
}

foreach ($scores as $name => $score) {
  echo "$name received a score of $score.\n";
}
